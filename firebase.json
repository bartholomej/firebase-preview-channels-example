{
  "hosting": [{
    "target": "previewchannels-app",
    "public": "functions/dist/previewchannels-app/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.original.html"
      }
    ]
  }, {
    "target": "ssr",
    "public": "functions/dist/previewchannels-app/browser",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "function": "ssr"
      }
    ]
  }],
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" install",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "target": "ssr",
    "source": "functions"
  }
}
